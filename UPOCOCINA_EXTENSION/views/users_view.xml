<odoo>
    <record id="gym_expansion_users_form_view" model="ir.ui.view">
        <field name="name">gym_expansion.users.form</field>
        <field name="model">gym.users</field>
        <field name="inherit_id" ref="gym.gym_users_form_view" />
        <field name="arch" type="xml">
            <field name="classes_ids" position="after">
                <field name="materiales_ids" />
            </field>
        </field>
    </record>

    <record id="users_list_action" model="ir.actions.act_window">
        <field name="name">Users</field>
        <field name="res_model">gym.users</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">Create the first user</p>
        </field>
    </record>
</odoo>